{
  "name": "authier-web-extension",
  "displayName": "Authier",
  "version": "1.0.21",
  "description": "Authier web Extension for all our supported browsers",
  "license": "AGPL-3.0-or-later",
  "webExt": {
    "sourceDir": "dist/"
  },
  "scripts": {
    "prodBuild": "AUTHIER_ENV=prod webpack --config webpack.prod.js",
    "devBuild": "AUTHIER_ENV=dev webpack --config webpack.dev.js",
    "newTag": "npm version patch && git add package.json && npx tsx ./scripts/updateManifestVersion.ts && npx tsx ./scripts/newTag.ts",
    "clean": "rm -rf dist/js",
    "checkBuildOutput": "node ./check-es6.js",
    "dev": "NODE_ENV=development webpack -w --config webpack.dev.js",
    "tsc": "tsc",
    "lingui:extract": "lingui extract",
    "lingui:compile": "lingui compile",
    "test": "echo",
    "playwright:run": "npx playwright test",
    "test2": "jest --config=jest.config.js",
    "lint": "eslint --fix -c ../.eslintrc.js \"src/**/*.ts*\"",
    "gen": "graphql-codegen",
    "test:run": "cd .. && cd backend && pnpm gen && pnpm dev",
    "uploadApp": "dotenv chrome-webstore-upload upload --source dist.zip --extension-id $EXTENSION_ID --client-id $CLIENT_ID --client-secret $CLIENT_SECRET --refresh-token $REFRESH_TOKEN",
    "publishApp": "dotenv chrome-webstore-upload publish --source dist.zip --extension-id padmmdghcflnaellmmckicifafoenfdi"
  },
  "keywords": [
    "react",
    "typescript",
    "chrome",
    "extension",
    "boilerplate"
  ],
