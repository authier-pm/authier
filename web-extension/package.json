    "@types/react-dom": "^19.1.5",
    "@types/react-test-renderer": "^19.1.0",
    "@types/webextension-polyfill": "^0.12.3",
    "@vitejs/plugin-react": "^4.4.1",
    "@vitest/coverage-v8": "^3.1.3",
    "@vitest/ui": "^3.1.3",
    "acorn": "^8.14.1",
    "babel-core": "^6.26.3",
    "babel-loader": "^10.0.0",
